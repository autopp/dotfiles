# setting of enviroment variable

export BASHRC=~/.bashrc

export PATH=$PATH:~/bin
export LD_LIBRARY=~/bin
export C_INCLUDE_PATH=$C_INCLUDE_PATH:~/.rbenv/versions/2.1.2/include/ruby-2.1.0:/home/auto/.rbenv/versions/2.1.2/include/ruby-2.1.0/x86_64-linux

export PATH=$LUCA/bin:$PATH
export PYTHONPATH=~/basil-read-only:$PYTHONPATH
export PATH=$PATH:~/basil-read-only/basil/apps
export TSOCKS_CONF_FILE=$HOME/.tsocks

export LUA_LDFLAGS='-llua -ldl -lm'
